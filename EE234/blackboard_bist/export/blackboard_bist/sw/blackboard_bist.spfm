<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="blackboard_bist"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
blackboard_bist
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="blackboard_bist">
        <sdx:configuration sdx:name="blackboard_bist"
                           sdx:displayName="blackboard_bist"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>blackboard_bist</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="blackboard_bist/boot/blackboard_bist.bif"
                            sdx:readme="blackboard_bist/boot/generic.readme"
                            sdx:qemuArguments="blackboard_bist/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="blackboard_bist/boot/blackboard_bist.bif"
                            sdx:readme="blackboard_bist/boot/generic.readme"
                            sdx:qemuArguments="blackboard_bist/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="blackboard_bist/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="blackboard_bist/standalone_domain/bspinclude/include"
                        sdx:bspConfig="blackboard_bist/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
